-----------------------------------------------------------------------------------------------------------------------------------------------------










class Solution:
    def printTriangle(self, N):
        # Code here
        k=1
        for i in range(1,N+1):
            for j in range(1,i+1):
                print(k+j-1, end=' ')
            k=k+j
            print()
