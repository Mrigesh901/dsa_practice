class node:
    def __init__(data):
        self.data = data
        self.next = None
'''
class Solution:
    #  Should return data of middle node. If linked list is empty, then  -1
    def findMid(self, head):
        # Code here
        # return the value stored in the middle node
        if head==None:
            return -1
        curr = head
        c = 1
        while curr.next!=None:
            curr = curr.next
            c+=1
        if c%2==0:
            x = c//2+1
        else:
            x = (c+1)//2
        curr = head
        
        for i in range(x-1):
            curr = curr.next
        return curr.data
